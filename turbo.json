{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "globalEnv": ["NODE_ENV", "VITE_API_URL", "NEXT_PUBLIC_API_URL"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**", "!.next/cache/**",
        ".solid/**"
      ]
    },
    "server-actix#build": {
      "dependsOn": ["web-yew#build"]
    },
    "solid:build": {
      "dependsOn": ["web-solid-start#build"],
      "outputs": [
        ".solid/**"
      ]
    },
    "next:build": {
      "dependsOn": ["web-nextjs#build"],
      "outputs": [
        ".next/**", "!.next/cache/**"
      ]
    },
    "web-yew#build": {
      "dependsOn": [],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
