{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Solid",
  "extends": "./base.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "jsxImportSource": "solid-js",
    "jsx": "preserve"
  },
  "exclude": ["node_modules"]
}
