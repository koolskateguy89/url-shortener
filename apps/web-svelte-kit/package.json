{
	"name": "web-svelte-kit",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "pnpm test:integration && pnpm test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check --ignore-path=../../.prettierignore . **.svelte && eslint .",
		"format": "prettier --plugin-search-dir . --write --ignore-path=../../.prettierignore . **.svelte",
		"test:integration": "echo TODO fix playwright && exit && playwright test",
		"test:unit": "vitest run"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^5.0.4",
		"@neoconfetti/svelte": "^1.0.0",
		"@playwright/test": "^1.35.1",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.21.0",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.61.0",
		"@typescript-eslint/parser": "^5.61.0",
		"api": "workspace:*",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.32.2",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.3",
		"svelte-check": "^3.4.4",
		"tailwindcss": "^3.3.2",
		"tsconfig": "workspace:*",
		"tslib": "^2.6.0",
		"typescript": "^5.1.6",
		"ui": "workspace:ui-svelte@*",
		"vite": "^4.3.9",
		"vitest": "^0.32.4"
	},
	"type": "module"
}
